<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <display-name>template1</display-name>
            <url-pattern>/showNewClient.jsp</url-pattern>
            <url-pattern>/showLogin.jsp</url-pattern>
            <url-pattern>/showListProducts.jsp</url-pattern>
            <url-pattern>/NewProduct.jsp</url-pattern>
            <url-pattern>/showAddMoneyToClient.jsp</url-pattern>
            <url-pattern>/showListClients.jsp</url-pattern>
            <url-pattern>/showListTakeProducts.jsp</url-pattern>
            <url-pattern>/showClientInfo.jsp</url-pattern>
            <url-pattern>/AddCountProduct.jsp</url-pattern>
            <url-pattern>/index.jsp</url-pattern>
            <url-pattern>/showAddCountProduct.jsp</url-pattern>
            <url-pattern>/showTakeProductToClient.jsp</url-pattern>
            <url-pattern>/showProductInfo.jsp</url-pattern>
            <url-pattern>/ShowAddMoneyToClient.jsp</url-pattern>
            <page-encoding>UTF-8</page-encoding>
            <include-prelude>/WEB-INF/folder/header.jspf</include-prelude>
            <include-coda>/WEB-INF/folder/footer.jspf</include-coda>
        </jsp-property-group>
    </jsp-config>
</web-app>
